ORG 0000H
MOV DPTR, #0050H
MOV A, #0F7H
MOV P0, A
AGAIN: MOV ACC, #00H
NEXT: MOV B, A
	  MOVC A, @A+DPTR
	  MOV P1, A
	  ACALL DELAY
	  MOV A, B
	  INC A
	  CJNE A, #0AH, NEXT
	  SJMP AGAIN
DELAY: MOV R4, #00H
HERE1: MOV R3, #00H
HERE2: MOV R2, #00H
HERE3: DJNZ R2, HERE3
	  DJNZ R3, HERE2
	  DJNZ R4, HERE1
	  RET
	  ORG 50H
	  DB 3FH, 06H, 5BH, 4FH, 66H, 6DH, 70H, 07H, 7FH, 6FH
END